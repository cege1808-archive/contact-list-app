<h1>Contact List</h1>
<span class="error"></span>
<div class="add-contact col-xs-offset-10">
  <button type="button" class="btn btn-default add"data-toggle="tooltip" data-placement="left" title="Add a Friends">
    <span class="glyphicon glyphicon-plus"></span>
  </button>
    <button type="button" class="btn btn-default cancel"data-toggle="tooltip" data-placement="left" title="Close">
    <span class="glyphicon glyphicon-remove-circle"></span>
  </button>

  <form>
    <div>
      <label for="first_name">First Name</label><br/>
      <input type="text" name="first_name" />
    </div>

    <div>
      <label for="last_name">Last Name</label><br/>
      <input type="text" name="last_name" />
    </div>

    <div>
      <label for="email">Email</label><br/>
      <input type="text" name="email" />
    </div>

    <div>
      <label for="phone">Phone Number</label><br/>
      <input type="text" name="phone" />
    </div>

    <div>
      <label for="birthday">Birthday</label><br/>
      <input type="date" name="birthday" />
    </div>

    <button>Add</button>
  </form>

</div>


<table class="table table-hover contact-list col-xs-3">
  <thead></thead>
  <tbody></tbody>
</table>
