<h1 class="col-xs-5 brand">Contact List</h1>
<span class="error"></span>

<div class="add-contact col-xs-offset-3">
  <button type="button" class="btn btn-default add"data-toggle="tooltip" data-placement="left" title="Add a Friends">
    <span class="glyphicon glyphicon-plus"></span>
  </button>
  <button type="button" class="btn btn-default cancel"data-toggle="tooltip" data-placement="left" title="Close">
    <span class="glyphicon glyphicon-remove-circle"></span>
  </button>

  <form>
    <div class="form-group">
      <label for="first_name">First Name</label><br/>
      <span class="error_first_name"></span>
      <input type="text" class="form-control" name="first_name" />
    </div>

    <div class="form-group">
      <label for="last_name">Last Name</label><br/>
      <span class="error_last_name"></span>
      <input type="text" class="form-control" name="last_name" />
    </div>

    <div class="form-group">
      <label for="email">Email</label><br/>
      <span class="error_email"></span>
      <input type="text" class="form-control" name="email" />
    </div>

    <div class="form-group">
      <label for="phone">Phone Number</label><br/>
      <span class="error_phone"></span>
      <input type="text" class="form-control" name="phone" />
    </div>

    <div class="form-group">
      <label for="birthday">Birthday</label><br/>
      <span class="error_birthday"></span>
      <input type="date" class="form-control" name="birthday" />
    </div>

    <button class="btn btn-default">Add</button>
  </form>
</div>


<div class="search-contact">
  <button type="button" class="btn btn-default search"data-toggle="tooltip" data-placement="left" title="Add a Friends">
    <span class="glyphicon glyphicon-search"></span>
  </button>
  <button type="button" class="btn btn-default cancel"data-toggle="tooltip" data-placement="left" title="Close">
    <span class="glyphicon glyphicon-remove-circle"></span>
  </button>

  <form>
    <div class="form-group">
      <label for="keyword">Keyword</label><br/>
      <input type="text" class="form-control" name="keyword" />
    </div>

    <button class="btn btn-default">Search</button>
  </form>
</div>

<div class="all-contact">
  <button type="button" class="btn btn-default search"data-toggle="tooltip" data-placement="left" title="Add a Friends">
    <span class="glyphicon glyphicon-ice-lolly"></span>
  </button>
</div>



<table class="table table-hover contact-list col-xs-3">
  <thead></thead>
  <tbody></tbody>
</table>
